{
  "name": "@selemondev/create-vue3-app",
  "version": "0.0.11",
  "description": "The Next Generation Vue Scaffolding Tool âœ¨",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "esno src/index.ts",
    "build:package": "tsup-node src/index.ts --format cjs,esm --clean --dts --minify --shims",
    "generate:release": "pnpx changelogen@latest --release",
    "package:beta": "pnpm build:package && npm publish --tag beta",
    "package": "pnpm build:package && npm publish --access=public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "@selemondev/create-vue3-app": "./dist/index.js"
  },
  "files": [
    "dist",
    "src",
    "template"
  ],
  "keywords": [
    "@selemondev/create-vue3-app",
    "Vue CLI",
    "Vue 3 CLI",
    "Vite CLI",
    "Vite",
    "Vue 3",
    "VueUse",
    "TypeScript"
  ],
  "author": "Selemondev",
  "license": "MIT",
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.9.0",
    "@types/prompts": "^2.4.9",
    "@types/validate-npm-package-name": "^4.0.2",
    "conf": "^15.0.2",
    "esno": "^4.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "ejs": "^3.1.9",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1",
    "picocolors": "^1.1.1",
    "prettier": "^3.2.5",
    "prompts": "^2.4.2",
    "validate-npm-package-name": "^7.0.0"
  }
}
