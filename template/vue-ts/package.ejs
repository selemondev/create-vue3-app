{
  "name": "vite-template",
  "private": true,
  "version": "1.0.0",
  <% if (package === 'pnpm') { -%>
    "packageManager": "pnpm@8.15.4",
  <% } -%>
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    <% if (useEslint) { -%>
    <%- EslintScript %>
    <% } -%>
    <% if (useVitest) { -%>
    <%- VitestScript %>
    <% } -%>
    <% if (useTypeScript) { -%>
    <%- TypeScriptScript %>
    <% } -%>
  },
  "dependencies": {
    <%- constantProDeps %>
    <% if (useRouter) { -%>
    <%- Router %>
    <% } -%>
    <% if (useVueUse) { -%>
    <%- VueUse %>
    <% } -%>
    <% if (usePinia) { -%>
    <%- Pinia %>
    <% } -%>
    <% if (useTanStackVueQuery) { -%>
    <%- TanStackVueQuery %>
    <% } -%>
  },
  "devDependencies": {
    <%- constantDevDeps %>
    <% if (useTypeScript) { -%>
      <%- TypeScript %>
    <% } -%>
    <% if (useEslint) { -%>
      <%- Eslint %>
    <% } -%>
    <% if (useTailwind) { -%>
      <%- Tailwind %>
    <% } -%>
    <% if (useJavaScript) { -%>
      <%- JavaScript %>
    <% } -%>
    <% if (useVitest) { -%>
      <%- Vitest %>
    <% } -%>
    <% if (useDevTool) { -%>
      <%- DevTool %>
    <% } -%>
    "vite": "^5.0.11",
    "@vitejs/plugin-vue": "^5.0.3"
  }
}
